#include <stdio.h>
#include <math.h>
#include <string.h>

int main (){
    int numero[2],i;
    for (  i = 0; i < 2; i++)
    {
       printf("Por favor me informe o numero %d :",i+1);
       scanf("%d",&numero[i]); 
    }
    if (numero[0]>numero [1])
    {
        printf("Numero %d e o maior",numero[0]);
    }else if (numero[0]== numero[1])
    {
      printf("Numeros iguais"); 
    }else
    {
        printf("Numero %d e o maior",numero[1]);
    }
    return 0;
}
