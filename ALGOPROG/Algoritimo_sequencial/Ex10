#include <stdio.h>
#include <stdlib.h>

int main(){
int y[2] ,x[2],i;
float resultado;
for ( i = 0; i < 2; i++)
{
    printf("Por favor me forneca o x%d :",i+1);
    scanf("%d",&x[i]);
    printf("Por favor me forneca o y%d :",i+1);
    scanf("%d",&y[i]);
}
resultado = sqrt(pow(x[1]-x[0],2)+pow(y[1]-y[0],2));
printf("A distancia e %.2f",resultado);
}
