#include <stdio.h>

int main(void)
{

float notas[30];
int alunos;
int i;

   printf("Digite quantos alunos sao:\n");
   scanf("%d\n", &alunos);
   
for ( i = 1; i <= alunos; i++){
   printf("Digite a %d nota:\n", i);
   scanf("%f\n", &notas[i]);
}
for ( i=1; i <= alunos; i++){
   printf("Nota %d: %.1f\n", i, notas[i]);
}


    return 0;
}
